<div class="container d-flex justify-content-center align-items-center">
  <div class="row row-col-md-6 row-offset-md-3 row-col-xl-4 row-offset-xl-4">
    <div class="card card shadow">
      <img
        src="data:image/png;base64,"
        class="card-img-top"
        alt="..."
      />
      <div class="card-body" style="width: 22rem">
        <h5 class="card-title">Criar Conta</h5>
        <form action="/register" method="POST" class="validate-form" novalidate>
          <div class="mb-3">
            <label class="form-label" for="username">Usuario</label>
            <input
              class="form-control"
              type="text"
              id="username"
              name="username"
              required
              autofocus
            />
            <div class="valid-feedback">Looks good!</div>
          </div>
          <div class="mb-3">
            <label class="form-label" for="email">Email</label>
            <input
              class="form-control"
              type="email"
              id="email"
              name="email"
              required
            />
            <div class="valid-feedback">Looks good!</div>
          </div>
          <div class="mb-3">
            <label class="form-label" for="password">Senha</label>
            <input
              class="form-control"
              type="password"
              id="password"
              name="password"
              required
            />
            <div class="valid-feedback">Looks good!</div>
          </div>
          <div class="d-grid gap-2">
            <button class="btn btn-dark">Criar</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
